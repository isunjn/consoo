{
  "name": "consoo",
  "version": "0.0.2",
  "description": "A set of helper functions for quick and dirty frontend debugging",
  "keywords": [
    "debugging",
    "console",
    "devtools",
    "debug",
    "typescript",
    "log"
  ],
  "author": "isunjn",
  "homepage": "https://github.com/isunjn/consoo",
  "bugs": {
    "url": "https://github.com/isunjn/consoo/issues"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/consoo.d.ts",
  "main": "./dist/consoo.umd.cjs",
  "module": "./dist/consoo.js",
  "exports": {
    ".": {
      "import": "./dist/consoo.js",
      "require": "./dist/consoo.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0"
  }
}
